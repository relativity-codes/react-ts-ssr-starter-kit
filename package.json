{
  "name": "react-ts-ssr-starter-kit",
  "version": "1.0.0",
  "description": "react starter kit with typescript and import alias like next js and tailwind setup using webpack and ssr for seo configs",
  "main": "server.js",
  "scripts": {
    "dev": "webpack --config webpack.client.js --mode development && webpack --config webpack.server.js --mode development",
    "start": "node dist/server.js",
    "build": "webpack --config webpack.client.js --mode production && webpack --config webpack.server.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relativity-codes/react-ts-ssr-starter-kit.git"
  },
