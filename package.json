  "bugs": {
    "url": "https://github.com/relativity-codes/react-ts-ssr-starter-kit/issues"
  },
  "homepage": "https://github.com/relativity-codes/react-ts-ssr-starter-kit#readme",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "express": "^4.19.2",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
